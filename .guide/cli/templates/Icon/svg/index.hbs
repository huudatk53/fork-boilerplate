import { config } from '../../ConfigProvider';

const icons = config.icons.reduce((_icons, icon) => {
  try {
    _icons[icon] = require(`./${icon}`).default;
    return _icons;
  } catch (e) {
    console.error(`${icon} not exsist!`);
  }
}, {});

export default icons;
